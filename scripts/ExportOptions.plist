<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Store distribution (includes TestFlight) -->
    <key>method</key>
    <string>app-store-connect</string>

    <!-- Your Apple Developer Team ID -->
    <key>teamID</key>
    <string>UJ4482ZF9C</string>

    <!-- Use automatic signing (matches project settings) -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Upload debug symbols for crash reports -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Upload bitcode if available -->
    <key>uploadBitcode</key>
    <false/>

    <!-- Destination for the export -->
    <key>destination</key>
    <string>upload</string>

    <!-- Automatically manage signing -->
    <key>signingCertificate</key>
    <string>Apple Distribution</string>

    <!-- Generate app store info -->
    <key>generateAppStoreInformation</key>
    <false/>
</dict>
</plist>
